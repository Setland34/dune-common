{
  "analyze": [
    "--disable", "default",
    "--analyzers=clang-tidy",
    "--file=*/dune-common/*.cc",
    "--skip", ".codechecker/skipfile",
    "--enable", "misc-definitions-in-headers",
    "--disable", "Wdeprecated-copy",
    "--disable", "Wunused"
  ],
  "parse": [
    "--file=*/dune-common/*"
  ]
}
